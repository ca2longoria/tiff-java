
public class NinePiece
{
	/**
	 * @author:		Cesar A. Longoria II
	 */
	public static void main(String[] args)
	{
		declaration();
		
		puzzlePiece[][] thePuzzle = new puzzlePiece[3][3];
		
		//mix 'em up
		mixEm();
		//throw 'em on
		//check 'em
		
	}

	public static void declaration()
	{
		//  Here I declare all the puzzle pieces.
		puzzlePiece num1 = new puzzlePiece("C", "H", "d", "c");
		puzzlePiece num2 = new puzzlePiece("D", "C", "c", "d");
		puzzlePiece num3 = new puzzlePiece("S", "D", "s", "h");
		puzzlePiece num4 = new puzzlePiece("C", "H", "s", "h");
		puzzlePiece num5 = new puzzlePiece("S", "D", "h", "d");
		puzzlePiece num6 = new puzzlePiece("S", "S", "h", "c");
		puzzlePiece num7 = new puzzlePiece("H", "D", "d", "h");
		puzzlePiece num8 = new puzzlePiece("H", "C", "s", "c");
		puzzlePiece num9 = new puzzlePiece("H", "D", "c", "c");
	}
	
	public static void mixEm()
	{
		num1.print();
		
	}
}
